<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0"
      prefWidth="350.0" style="-fx-background-radius: 8;" xmlns="http://javafx.com/javafx/18"
      xmlns:fx="http://javafx.com/fxml/1">
   <Text fx:id="txtCar" strokeType="OUTSIDE" strokeWidth="0.0" text="Toyota Vios" StackPane.alignment="TOP_LEFT">
      <font>
         <Font name="System Bold" size="16.0"/>
      </font>
      <StackPane.margin>
         <Insets left="10.0" top="105.0"/>
      </StackPane.margin>
   </Text>
   <Text fx:id="txtLocation" strokeType="OUTSIDE" strokeWidth="0.0" text="Kuala Lumpur"
         StackPane.alignment="TOP_LEFT">
      <font>
         <Font size="13.0"/>
      </font>
      <StackPane.margin>
         <Insets left="10.0" top="125.0"/>
      </StackPane.margin>
   </Text>
   <ImageView fx:id="picCar" fitHeight="80.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true"
              StackPane.alignment="TOP_LEFT">
      <image>
         <Image url="@img/carVios.png"/>
      </image>
      <StackPane.margin>
         <Insets left="85.0" top="20.0"/>
      </StackPane.margin>
   </ImageView>
   <Button fx:id="btnView" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
           mnemonicParsing="false" prefHeight="35.0" prefWidth="90.0"
           style="-fx-background-color: #000000; -fx-background-radius: 8;" text="View" textFill="WHITE"
           StackPane.alignment="TOP_LEFT">
      <font>
         <Font name="System Bold" size="17.0"/>
      </font>
      <StackPane.margin>
         <Insets left="250.0" top="105.0"/>
      </StackPane.margin>
   </Button>
   <HBox prefHeight="100.0" prefWidth="200.0"/>
</HBox>
